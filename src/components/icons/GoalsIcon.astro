---
import Icon12ResponsibleConsumption from '@/content/site/images/sd/Icon12ResponsibleConsumption.astro'; 
import Icon13Climate from '@/content/site/images/sd/Icon13Climate.astro'; 
import Icon14LifeBelowWater from '@/content/site/images/sd/Icon14LifeBelowWater.astro'; 
import Icon17Partnerships from '@/content/site/images/sd/Icon17Partnerships.astro';

export interface Props {
  icon: string;
}

const { icon } = Astro.props;
---

{icon === "ResponsibleConsumption" && <Icon12ResponsibleConsumption title="Responsible Consumption" />}
{icon === "Climate" && <Icon13Climate title="Climate" />}
{icon === "LifeBelowWater" && <Icon14LifeBelowWater title="Life Below Water" />}
{icon === "Partnerships" && <Icon17Partnerships title="Partnerships" />}
